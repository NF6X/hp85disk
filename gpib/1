typedef union {
        uint8_t b;
        uint16_t w;
        uint32_t l;
} val_t;

typedef struct 
{
	char *name;
	int  tok;
} token_t;

enum {
    TOK_ACCESS_TIME,
    TOK_ADDRESS,
    TOK_AMIGO,
    TOK_BLOCKS,
    TOK_BLOCK_TIME,
    TOK_BUFFERED_BLOCKS,
    TOK_BURST_SIZE,
    TOK_BYTES_PER_BLOCK,
    TOK_BYTES_PER_SECTOR,
    TOK_CONFIG,
    TOK_CONTINUOUS_TRANSFER_RATE,
    TOK_CONTROLLER,
    TOK_CS80,
    TOK_CYLINDERS,
    TOK_DEBUG,
    TOK_DEVICE_NUMBER,
    TOK_DRIVE,
    TOK_END,
    TOK_FILE,
    TOK_FIXED_VOLUMES,
    TOK_GEOMETRY,
    TOK_HEADER,
    TOK_HEADS,
    TOK_ID,
    TOK_INTERLEAVE,
    TOK_MAX_BLOCK_NUMBER,
    TOK_MAX_CYLINDER,
    TOK_MAX_HEAD,
    TOK_MAXIMUM_INTERLEAVE,
    TOK_MAX_SECTOR,
    TOK_OPTIMAL_RETRY_TIME,
    TOK_PPR,
    TOK_PRINTER,
    TOK_REMOVABLE_VOLUMES,
    TOK_SECTORS_PER_TRACK,
    TOK_SS80,
    TOK_SS80_DEFAULT,
    TOK_TRANSFER_RATE,
    TOK_TYPE,
    TOK_UNIT,
    TOK_UNITS_INSTALLED,
    TOK_UNIT_TYPE,
    TOK_VOLUME,
	TOK_INVALID = -1
};


